{
  "name": "ionic-angular-starter",
  "description": "An Ionic project",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev": "ng run app:serve --host=localhost --port=4200",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format:ts": "prettier --write 'src/**/*.{js,ts,scss,css,less,jsx,tsx,json}'",
    "format:html": "prettier --write 'src/**/*.{html,xml}'",
    "format": "npm run format:ts && npm run format:html",
    "register-git-hooks": "npx simple-git-hooks"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~13.3.3",
    "@angular/core": "~13.3.3",
    "@angular/forms": "~13.3.3",
    "@angular/platform-browser": "~13.3.3",
    "@angular/platform-browser-dynamic": "~13.3.3",
    "@angular/router": "~13.3.3",
    "@auth0/angular-jwt": "^5.0.2",
    "@capacitor/android": "3.4.3",
    "@capacitor/app": "1.1.1",
    "@capacitor/core": "3.4.3",
    "@capacitor/haptics": "1.1.4",
    "@capacitor/keyboard": "1.2.2",
    "@capacitor/status-bar": "1.0.8",
    "@ionic/angular": "^6.1.0",
    "@ionic/storage-angular": "^3.0.6",
    "@ng-web-apis/common": "^2.0.0",
    "appwrite": "^7.0.0",
    "cordova-sqlite-storage": "^6.0.0",
    "localforage-cordovasqlitedriver": "^1.8.0",
    "lodash-es": "^4.17.21",
    "rxjs": "^7.5.5",
    "rxjs-state-subject": "^3.0.0",
    "swiper": "^8.1.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.3",
    "@angular-eslint/builder": "~13.2.1",
    "@angular-eslint/eslint-plugin": "~13.2.1",
    "@angular-eslint/eslint-plugin-template": "~13.2.1",
    "@angular-eslint/template-parser": "~13.2.1",
    "@angular/cli": "~13.3.3",
    "@angular/compiler": "~13.3.3",
    "@angular/compiler-cli": "~13.3.3",
    "@angular/language-service": "~13.3.3",
    "@capacitor/cli": "3.4.3",
    "@ionic/angular-toolkit": "^6.1.0",
    "@tailwindcss/aspect-ratio": "latest",
    "@tailwindcss/line-clamp": "latest",
    "@tailwindcss/typography": "latest",

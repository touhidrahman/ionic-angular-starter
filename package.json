    "ngx-tailwind": "^3.0.0",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "prettier-eslint": "^13.0.0",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "protractor": "~7.0.0",
    "simple-git-hooks": "^2.7.0",
    "tailwindcss": "^3.0.24",
    "ts-node": "~8.3.0",
    "typescript": "~4.4.4"
  },
  "lint-staged": {
    "*.{scss,css,less,json}": "prettier --write",
    "*.{html,xml}": "prettier --write"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  }
}
